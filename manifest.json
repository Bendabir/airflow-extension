{
    "manifest_version": 2,
    "name": "Airflow DAG auto-refresh",
    "description": "Refreshes the Airlfow DAG run view automagically.",
    "version": "0.1.0",
    "icons": {
        "48": "assets/icons/auto_refresh_icon.png"
    },
    "permissions": [
        "*://*/*"
    ],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "matches": ["*://*/*"],
        "run_at": "document_end",
        "js": [
            "auto_refresh.js"
        ]
    }]
}
